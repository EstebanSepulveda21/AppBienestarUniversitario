<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Reserve_Without">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/arrow_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="83dp"
            android:clickable="true"
            app:srcCompat="@drawable/ic_arrow_back"
            android:onClick="btn_back_arrow_without_reserve"/>

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="145dp"
            android:layout_marginTop="83dp"
            android:text="@string/bookings_title"
            android:textColor="@color/black"
            android:textSize="20dp" />


        <ImageView
            android:layout_width="180dp"
            android:layout_height="180dp"
            android:src="@drawable/ic_exclamation_dialog_reserve"
            android:layout_marginTop="180dp"
            android:layout_marginStart="100dp"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/not_bookings_title"
            android:layout_marginLeft="120dp"
            android:layout_marginTop="380dp"
            android:textColor="@color/black"
            />

        <ImageView
            android:layout_width="160dp"
            android:layout_height="150dp"
            android:src="@drawable/ic_unibague"
            android:layout_marginTop="580dp"
            android:layout_marginStart="120dp"
            android:layout_gravity="center"
            app:tint="@color/main_color_university" />


    </RelativeLayout>

</RelativeLayout>